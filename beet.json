{
    "name": "FireflyLib",
    "description": "A function driven datapack particle library.",
    "version": "v1.0.0",
    "author": "Trplnr",
    "require": ["bolt"],
    "data_pack": {
      "load": ["./src"],
      "pack_format": 71
    },
    "resource_pack": {
      "load": ["./src"],
      "pack_format": 55
    },
    "pipeline": ["mecha"],
    "output": "./build",
    "meta": {
      "bolt": {"entrypoint":"fflib:*"}
    }
}